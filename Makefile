include ./Makefile.variables




all: 
	reset
	@echo "Common target is not defined."
	@echo "Please, specify make target."

execute:
	reset
	@echo "'execute' target is not defined."
	@echo "Please, specify make target."

debug:
	reset
	@echo "'debug' target is not defined."
	@echo "Please, specify make target."

config:
	reset

clean: config \
	$(PROJECT_HOOKS_NAME)_clean \
	$(PROJECT_FRAMEWORK_NAME)_clean \
	$(PROJECT_APPLICATION_NAME)_clean \
	$(PROJECT_SERVICEBROCKER_NAME)_clean \
	$(PROJECT_APP0_NAME)_clean \

build: config \
	$(PROJECT_HOOKS_NAME)_build \
	$(PROJECT_FRAMEWORK_NAME)_build \
	$(PROJECT_APPLICATION_NAME)_build \
	$(PROJECT_SERVICEBROCKER_NAME)_build \
	$(PROJECT_APP0_NAME)_build \

clean_build: clean build

generate: config \
	$(PROJECT_HOOKS_NAME)_generate \
	$(PROJECT_FRAMEWORK_NAME)_generate \
	$(PROJECT_APPLICATION_NAME)_generate \
	$(PROJECT_SERVICEBROCKER_NAME)_generate \
	$(PROJECT_APP0_NAME)_generate \

info: config \
	$(PROJECT_HOOKS_NAME)_info \
	$(PROJECT_FRAMEWORK_NAME)_info \
	$(PROJECT_APPLICATION_NAME)_info \
	$(PROJECT_SERVICEBROCKER_NAME)_info \
	$(PROJECT_APP0_NAME)_info \

archive: clean
	@$(TAR) -cvf $(ROOT_DIR)/../$(PROJECT_NAME)_$(DATE)_$(TIME).tar ../$(PROJECT_NAME)



$(PROJECT_FRAMEWORK_NAME)_build:
	$(MAKE) -C $(PROJECT_FRAMEWORK_DIR) build $(FRAMEWORK_MAKE_PARAMS)

$(PROJECT_FRAMEWORK_NAME)_generate:
	$(MAKE) -C $(PROJECT_FRAMEWORK_DIR) generate $(FRAMEWORK_MAKE_PARAMS)

$(PROJECT_FRAMEWORK_NAME)_clean:
	$(MAKE) -C $(PROJECT_FRAMEWORK_DIR) clean $(FRAMEWORK_MAKE_PARAMS)

$(PROJECT_FRAMEWORK_NAME)_info:
	$(MAKE) -C $(PROJECT_FRAMEWORK_DIR) info $(FRAMEWORK_MAKE_PARAMS)

$(PROJECT_FRAMEWORK_NAME)_archive:
	$(MAKE) -C $(PROJECT_FRAMEWORK_DIR) archive $(FRAMEWORK_MAKE_PARAMS)



$(PROJECT_APPLICATION_NAME)_build:
	$(MAKE) -C $(PROJECT_APPLICATION_DIR) build $(APPLICATION_MAKE_PARAMS)

$(PROJECT_APPLICATION_NAME)_generate:
	$(MAKE) -C $(PROJECT_APPLICATION_DIR) generate $(APPLICATION_MAKE_PARAMS)

$(PROJECT_APPLICATION_NAME)_clean:
	$(MAKE) -C $(PROJECT_APPLICATION_DIR) clean $(APPLICATION_MAKE_PARAMS)

$(PROJECT_APPLICATION_NAME)_info:
	$(MAKE) -C $(PROJECT_APPLICATION_DIR) info $(APPLICATION_MAKE_PARAMS)

$(PROJECT_APPLICATION_NAME)_archive:
	$(MAKE) -C $(PROJECT_APPLICATION_DIR) archive $(APPLICATION_MAKE_PARAMS)

$(PROJECT_APPLICATION_NAME)_execute:
	$(MAKE) -C $(PROJECT_APPLICATION_DIR) execute $(APPLICATION_MAKE_PARAMS)

$(PROJECT_APPLICATION_NAME)_debug:
	$(MAKE) -C $(PROJECT_APPLICATION_DIR) debug $(APPLICATION_MAKE_PARAMS)

$(PROJECT_APPLICATION_NAME)_disassm:
	$(MAKE) -C $(PROJECT_APP0_DIR) disassm $(APP0_MAKE_PARAMS)



$(PROJECT_SERVICEBROCKER_NAME)_build:
	$(MAKE) -C $(PROJECT_SERVICEBROCKER_DIR) build $(SERVICEBROCKER_MAKE_PARAMS)

$(PROJECT_SERVICEBROCKER_NAME)_generate:
	$(MAKE) -C $(PROJECT_SERVICEBROCKER_DIR) generate $(SERVICEBROCKER_MAKE_PARAMS)

$(PROJECT_SERVICEBROCKER_NAME)_clean:
	$(MAKE) -C $(PROJECT_SERVICEBROCKER_DIR) clean $(SERVICEBROCKER_MAKE_PARAMS)

$(PROJECT_SERVICEBROCKER_NAME)_info:
	$(MAKE) -C $(PROJECT_SERVICEBROCKER_DIR) info $(SERVICEBROCKER_MAKE_PARAMS)

$(PROJECT_SERVICEBROCKER_NAME)_archive:
	$(MAKE) -C $(PROJECT_SERVICEBROCKER_DIR) archive $(SERVICEBROCKER_MAKE_PARAMS)

$(PROJECT_SERVICEBROCKER_NAME)_execute:
	$(MAKE) -C $(PROJECT_SERVICEBROCKER_DIR) execute $(SERVICEBROCKER_MAKE_PARAMS)

$(PROJECT_SERVICEBROCKER_NAME)_debug:
	$(MAKE) -C $(PROJECT_SERVICEBROCKER_DIR) debug $(SERVICEBROCKER_MAKE_PARAMS)

$(PROJECT_SERVICEBROCKER_NAME)_disassm:
	$(MAKE) -C $(PROJECT_APP0_DIR) disassm $(APP0_MAKE_PARAMS)



$(PROJECT_HOOKS_NAME)_build:
	$(MAKE) -C $(PROJECT_HOOKS_DIR) build $(HOOKS_MAKE_PARAMS)

$(PROJECT_HOOKS_NAME)_generate:
	$(MAKE) -C $(PROJECT_HOOKS_DIR) generate $(HOOKS_MAKE_PARAMS)

$(PROJECT_HOOKS_NAME)_clean:
	$(MAKE) -C $(PROJECT_HOOKS_DIR) clean $(HOOKS_MAKE_PARAMS)

$(PROJECT_HOOKS_NAME)_info:
	$(MAKE) -C $(PROJECT_HOOKS_DIR) info $(HOOKS_MAKE_PARAMS)

$(PROJECT_HOOKS_NAME)_archive:
	$(MAKE) -C $(PROJECT_HOOKS_DIR) archive $(HOOKS_MAKE_PARAMS)

$(PROJECT_HOOKS_NAME)_execute:
	$(MAKE) -C $(PROJECT_HOOKS_DIR) execute $(HOOKS_MAKE_PARAMS)



$(PROJECT_APP0_NAME)_build:
	$(MAKE) -C $(PROJECT_APP0_DIR) build $(APP0_MAKE_PARAMS)

$(PROJECT_APP0_NAME)_generate:
	$(MAKE) -C $(PROJECT_APP0_DIR) generate $(APP0_MAKE_PARAMS)

$(PROJECT_APP0_NAME)_clean:
	$(MAKE) -C $(PROJECT_APP0_DIR) clean $(APP0_MAKE_PARAMS)

$(PROJECT_APP0_NAME)_info:
	$(MAKE) -C $(PROJECT_APP0_DIR) info $(APP0_MAKE_PARAMS)

$(PROJECT_APP0_NAME)_archive:
	$(MAKE) -C $(PROJECT_APP0_DIR) archive $(APP0_MAKE_PARAMS)

$(PROJECT_APP0_NAME)_execute:
	$(MAKE) -C $(PROJECT_APP0_DIR) execute $(APP0_MAKE_PARAMS)

$(PROJECT_APP0_NAME)_debug:
	$(MAKE) -C $(PROJECT_APP0_DIR) debug $(APP0_MAKE_PARAMS)

$(PROJECT_APP0_NAME)_disassm:
	$(MAKE) -C $(PROJECT_APP0_DIR) disassm $(APP0_MAKE_PARAMS)







# Non-File Targets
.PHONY: all
