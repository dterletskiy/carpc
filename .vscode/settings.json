{
   "files.associations": {
      "*.tcc": "cpp",
      "deque": "cpp",
      "list": "cpp",
      "unordered_map": "cpp",
      "vector": "cpp",
      "string": "cpp"
   },

   "editor.tabSize": 3,
   "editor.rulers": [ 100 ],
   "editor.renderWhitespace": "all",
   "editor.detectIndentation": false,
   "editor.smoothScrolling": true,
   "editor.tabCompletion": "on",
   "editor.codeLens": false,
   "editor.fontSize": 15,

   "ccls.launch.command": "ccls"
}